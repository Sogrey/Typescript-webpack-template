{
  "name": "typescript-webpack-template",
  "version": "0.0.1",
  "description": "Sample template project for packaging typescript using webpack",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist dist.tsc",
    "clean:tsc": "rm -rf dist.tsc",
    "tsc": "yarn clean & npx tsc -p .",
    "build:dev": "yarn tsc & webpack --mode=development & yarn clean:tsc",
    "build": "yarn tsc & webpack --mode=production & yarn clean:tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sogrey/Typescript-webpack-template.git"
  },
  "keywords": [
    "TypeScript",
    "Webpack"
  ],
  "author": "Sogrey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sogrey/Typescript-webpack-template/issues"
  },
  "homepage": "https://github.com/Sogrey/Typescript-webpack-template#readme",
  "devDependencies": {
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}